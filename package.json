{
  "name": "wicked-tec1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --env=jsdom --colors --coverage spec --watch",
    "test-travis": "jest --env=jsdom --colors --coverage spec",
    "start": "parcel index.html --open -p 3000",
    "build": "parcel build index.html --public-url ./",
    "lh": "lighthouse-ci",
    "lighthouse": "lighthouse https://jhlagado.github.io/wicked-tec1/index.html  --view --output-path=./lighthouse.html"
  },
  "keywords": [
    "parcel",
    "wicked-elements"
  ],
  "author": "John Hardy",
  "license": "MIT",
  "dependencies": {
    "lit-html": "0.14.0",
    "nrf-intel-hex": "1.3.0",
    "wicked-elements": "0.3.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.10",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "jest": "23.6.0",
    "lighthouse-ci": "https://github.com/ebidel/lighthouse-ci",
    "parcel-bundler": "^1.9.7",
    "rimraf": "^2.6.2",
    "ts-jest": "23.10.5",
    "typescript": "3.2.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "setupTestFrameworkScriptFile": "./test.setup.ts",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
